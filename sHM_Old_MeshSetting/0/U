FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform (15 0 0);
    }

    outlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

    Ground
    {
        type            movingWallVelocity;
        value           uniform (15 0 0);
    }

    Sky
    {
        type slip;
    }

    SideWall
    {
        type slip;
    }

    Symmetry
    {
        type symmetryPlane;
    }

    Front_Tire
    {
        type            rotatingWallVelocity;
        origin          (0 0 0.197);
        axis            (0 -1 0);
        omega           constant 76.1;
    }

    Rear_Tire
    {
        type            rotatingWallVelocity;
        origin          (1.5505 0 0.197);
        axis            (0 -1 0);
        omega           constant 76.1;
    }

    FrBr
    {
        type            rotatingWallVelocity;
        origin          (0 0 0.197);
        axis            (0 -1 0);
        omega           constant 83.3;
    }

    RrBr
    {
        type            rotatingWallVelocity;
        origin          (1.5505 0 0.197);
        axis            (0 -1 0);
        omega           constant 83.3;
    }

    CARGroup
    {
        type            noSlip;
    }
}


